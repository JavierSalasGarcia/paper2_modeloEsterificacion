{
  "caso": "Caso 1 - Procesamiento GC-FID",
  "descripcion": "Procesamiento automatizado de datos cromatogr√°ficos",
  "objetivo": "Demostrar facilidad de uso vs Excel/MATLAB manual",
  "entrada": {
    "archivo_csv": "datos/experimento_60C.csv",
